data casedat_clean;
   set casedat(rename= (RaceandoriginrecodeNHWNHBNHAIA=RaceEth_code));
   by Patient_ID;
   
   Survival_months = Survival_months + .5; 
   
   *** EXPECTED MORTALITY RATE FOR THE LAST FOLLOW-UP INTERVAL (DEATH OR CENSOR) ***;
   *** NOTE THAT IT IS VERY IMPORTANT TO SET THE SURVIVAL INTERVALS TO 999 ON THE PARAMETERS TAB ***;
   *** IN SEER*STAT FOR THIS TO WORK PROPERLY. ***; 
   rate = -1 * log(FinalIntervalExpected_12_month); 
   label rate = 'Expected mortality rate at exit';

   if Survival_months  = 9999 then delete;

   /* delete DCO and autopsy cases */
   if Survival_months_flag in (8) then delete;

   *** OUTPUT ONE DIAGNOSIS RECORD PER PERSON ***;
   if first.Patient_ID then output; 
run;

